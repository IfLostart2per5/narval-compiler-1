qword <Integer | Decimal> :: label:hipotenusa:(qword <Integer | Decimal> :: a, qword <Integer | Decimal> :: b) => {
  qword <Integer | Decimal> :: x << ((a^2 + b^2) ^ (1/2));

  return x;
}

word Object<Integer | Decimal> :: valores << {
  primeiro: {
    a: 3,
    b: 4
  },
  segundo: {
    a: 3.14,
    b: 2.71
  }
};

word Object<Integer | Decimal> :: hipotenusas << {
  primeiro: hipotenusa:(valores.primeiro.a, valores.primeiro.b),
  segundo: hipotenusa:(valores.segundo.a, valores.segundo.b)
};

export:(hipotenusa, hipotenusas);
